{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "guestbook",
            "image": "vladmoldovan/guestbook",
            "hostname": "guestbook",
            "essential": false,
            "memory": 128
        },
        {
            "name": "nginx",
            "image": "vladmoldovan/guestbook_nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 443
                }
            ],
            "links": ["guestbook"],
            "memory": 128
        }
    ]
}